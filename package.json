{
  "version": "0.0.0",
  "repository": "NCI-GDC/portal-ui",
  "name": "portal-ui",
  "author": {
    "name": "OICR",
    "url": "https://github.com/NCI-GDC/"
  },
  "homepage": "https://portal.gdc.cancer.gov/",
  "scripts": {
    "start": "concurrently --kill-others 'npm run start-ui' 'npm run relay -- --watch'",
    "start-ui": "BROWSER=none jailbreak-react-scripts start",
    "build": "npm run relay && jailbreak-react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint .",
    "flow": "flow check",
    "relay": "relay-compiler --src src --schema ./data/schema.graphql",
    "schema": "node ./data/getSchema"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:5000",
      "pathRewrite": {
        "^/api": ""
      }
    }
  },
  "dependencies": {
    "@oncojs/react-lolliplot": "0.10.6",
    "@oncojs/sapien": "0.5.2",
    "@oncojs/survivalplot": "0.6.5",
    "babel-polyfill": "6.23.0",
    "blueimp-md5": "2.7.0",
    "color": "0.11.4",
    "concurrently": "3.5.0",
    "d3": "4.8.0",
    "download-svg": "0.1.1",
    "fbemitter": "2.1.1",
    "filesaver.js": "0.2.0",
    "filesize": "3.3.0",
    "glamor": "2.20.25",
    "import-inspector": "2.0.0",
    "innersvg-polyfill": "0.0.2",
    "js-cookie": "2.1.4",
    "jsurl": "0.1.5",
    "load-script": "1.0.0",
    "lodash-backports": "1.0.1",
    "memoizee": "0.4.5",
    "moment": "2.17.1",
    "oncogrid": "2.0.1",
    "pluralize": "7.0.0",
    "prop-types": "15.5.10",
    "query-string": "4.3.4",
    "queue": "4.2.1",
    "react": "15.5.4",
    "react-addons-shallow-compare": "15.4.2",
    "react-dates": "12.5.0",
    "react-dom": "15.5.4",
    "react-faux-dom": "3.1.0",
    "react-helmet": "5.1.3",
    "react-highlighter": "0.4.0",
    "react-icons": "2.2.5",
    "react-loadable": "4.0.2",
    "react-measure": "1.4.5",
    "react-modal": "1.7.7",
    "react-progress": "0.0.12",
    "react-redux": "5.0.4",
    "react-relay": "1.3.0",
    "react-relay-network-layer": "2.0.1",
    "react-router-dom": "4.1.2",
    "react-sizeme": "2.3.2",
    "react-sortable": "git+https://github.com/danielstocks/react-sortable.git",
    "recompose": "0.23.1",
    "redux": "3.6.0",
    "redux-actions": "1.2.2",
    "redux-api-middleware": "2.0.0-beta.2",
    "redux-persist": "4.8.0",
    "redux-thunk": "2.2.0",
    "url-join": "1.1.0"
  },
  "devDependencies": {
    "babel-plugin-import-inspector": "2.0.0",
    "babel-plugin-relay": "1.3.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "eslint-config-prettier": "2.1.1",
    "eslint-plugin-prettier": "2.1.1",
    "flow-bin": "0.53.1",
    "jailbreak-react-scripts": "0.0.2",
