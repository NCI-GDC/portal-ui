{
  "version": "0.2.0",
  "configurations": [
    {
    "name": "Chrome (brew)",
    "type": "chrome",
    "request": "launch",
    "runtimeExecutable":  "/opt/homebrew-cask/Caskroom/google-chrome/latest/Google Chrome.app/Contents/MacOS/Google Chrome",
    "url": "http://localhost:3000",
    "webRoot": "${workspaceRoot}/src",
    "userDataDir": "${workspaceRoot}/.vscode/chrome",
    "sourceMapPathOverrides": {
      "webpack:///src/*": "${webRoot}/*"
    }
  }]
}