{
	// See https://go.microsoft.com/fwlink/?LinkId=759670
	// for the documentation about the jsconfig.json format
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true,
		"baseUrl": "src/packages/"
	},
	"exclude": [
		"./node_modules",
		"bower_components",
		"jspm_packages",
		"tmp",
		"temp",
		".history",
		"dist"
	]
}
